{
  "name": "buffercursor.ts",
  "version": "1.0.3",
  "description": "A simple package that allows you to traverse a Buffer iteratively",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
