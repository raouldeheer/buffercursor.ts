  "bugs": {
    "url": "https://github.com/raouldeheer/buffercursor.ts/issues"
  },
  "homepage": "https://github.com/raouldeheer/buffercursor.ts#readme",
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "@types/node": "^22.5.1",
    "jest": "^29.1.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsup": "^7.1.0",
    "typescript": "^5.0.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/raouldeheer"
